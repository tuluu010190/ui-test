<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_TestConfig" />
			<class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_MemberManagement_Invite" />
		    <class name="org.exoplatform.selenium.platform.social.functional.SOC_Space_SpacesManagement" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>