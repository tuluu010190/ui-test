<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_TestConfig" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Event_Activity_Actions" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Event_Activity_Add" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_General" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Task_Details" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Event_Activity_Delete" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Event_Activity_Display" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Activity_Event_Activity_Recurring" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>