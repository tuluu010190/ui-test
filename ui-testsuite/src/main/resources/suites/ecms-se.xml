<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ECMS Functional Test" verbose="1" preserve-order="true">
	<test name="ecms-func-se" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<packages>
			<package
				name="org.exoplatform.selenium.platform.ecms.functional.siteexplorer.*" />
		</packages>
	</test>

	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>