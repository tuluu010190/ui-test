<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="gatein-func-part-1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.gatein.functional.managepages.Gatein_ManagePages_Create" />
			<class name="org.exoplatform.selenium.platform.gatein.functional.managepages.Gatein_ManagePages_Edit" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>