<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="social-sniff-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_TestConfig_3" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_Recurring" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_Event_Details" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_End_Date_Suggestion" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_Popover" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_View" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Event_Repeat" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>