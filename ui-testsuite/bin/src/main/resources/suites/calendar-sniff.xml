<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Platform Sniff Test">
	<test name="calendar-sniff">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Event" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Task" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Event_Recurring" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Calendar" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Category" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Publish_Activity" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Search" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_View" />
			<class
				name="org.exoplatform.selenium.platform.calendar.sniff.Calendar_Setting" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>