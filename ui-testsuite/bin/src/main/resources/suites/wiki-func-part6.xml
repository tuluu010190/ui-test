<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Functional Test" verbose="1" preserve-order="true">
	<test name="wiki-func" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.wiki.functional.WIKI_TestConfig_2" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Page_Permission_Add" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Page_Permission_Check_Permission" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Page_Permission_Delete" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Wiki_Permission_Add" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Wiki_Permission_Check_Permission" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_Permission_Wiki_Permission_Delete" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>