<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Functional Test" verbose="1" preserve-order="true">
	<test name="wiki-func" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.wiki.functional.WIKI_TestConfig" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_BasicAction_Add_AutoSave" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_BasicAction_Add_RichText" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_BasicAction_Add_SourceEditor" />
			<class name="org.exoplatform.selenium.platform.wiki.functional.Wiki_BasicAction_Add_Draft_Others_Template" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>