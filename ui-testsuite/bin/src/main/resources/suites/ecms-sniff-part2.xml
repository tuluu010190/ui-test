<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="ecms-sniff" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class
				name="org.exoplatform.selenium.platform.ecms.sniff.ECMS_TestConfig_Part2" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Admin" />
			<class
				name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_BasicAction" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Create" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Info" />

			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Search" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Setting" />

			<class
				name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Collaboration" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Info" />
			<class
				name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_PublishActivities" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Search" />
			<class name="org.exoplatform.selenium.platform.ecms.sniff.Ecms_SE_Setting" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>