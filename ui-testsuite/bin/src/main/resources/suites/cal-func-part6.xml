<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="calendar-sniff-part1" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_TestConfig_3" />
			<!-- <class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Search" />
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Task_End_Date_Suggestion" /> -->
			<class name="org.exoplatform.selenium.platform.calendar.functional.CAL_Task_Task_Details" />
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>